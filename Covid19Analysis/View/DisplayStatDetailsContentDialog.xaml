<ContentDialog
    x:Class="Covid19Analysis.View.DisplayStatDetailsContentDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Display Daily Statistic Details"
    PrimaryButtonText="Cancel"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick">

    <Grid >
        <TextBox x:Name="detailsTextBox" HorizontalAlignment="Left" Margin="12,67,0,0" VerticalAlignment="Top" TextWrapping="Wrap" Text="Details" Height="276" Width="485" IsReadOnly="True" AutomationProperties.Name="summaryTextBlock" FontFamily="Consolas"/>

    </Grid>
</ContentDialog>